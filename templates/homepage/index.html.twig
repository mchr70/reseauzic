{% extends 'base.html.twig' %}

{% block title %}Hello HomePageController!{% endblock %}

{% block body %}
    <h3>Derniers inscrits</h3>
    {% for user in users%}
        <li>{{ user.email }}</li>
    {% endfor %}

    <div class="jumbotron">
        {{ form_start(form) }}
        {{ form_row(form.zipCode) }}
        {{ form_row(form.city) }}
        <button class="btn btn-primary">Rechercher</button>
        {{ form_end(form) }}
    </div>
{% endblock %}
