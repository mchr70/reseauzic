{% extends 'base.html.twig' %}

{% block title %}Hello HomePageController!{% endblock %}

{% block body %}
    
    {% if users %}
        <h3>Derniers inscrits</h3>
        {% for user in users %}
            <li><a href="{{ path('member_profile', {"id":user.id}) }}">{{ user.username }}</a></li>
        {% endfor %}
    {% else %}
        Pas encore d'inscrits
    {% endif %}

{% endblock %}
